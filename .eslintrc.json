{
    "env": {
        "browser": true,
        "es6": true
    },
    "extends": [
        "google"
    ],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint"
    ],
    "rules": {
        "default-case": "warn",
        "max-len": "off",
        "semi": "warn",
        "require-jsdoc": "off",
        "no-const-assign": "warn",
        "no-this-before-super": "warn",
        "prettier/prettier": "warn",
        "object-curly-spacing": "warn",
        "linebreak-style": "off",
        "no-trailing-spaces": "warn",
        "padded-blocks": "warn",
        "func-style": "warn",
        "*": "warn",
        "no-unused-vars": "warn"
    }
}